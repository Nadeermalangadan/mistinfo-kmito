<!doctype html>
<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="ru" lang="ru">
<head profile="http://gmpg.org/xfn/11">
    {% hook 'html_head_begin' %}
    <meta http-equiv="X-UA-Compatible" content="IE=9" />
    <meta charset="utf-8">

    <link rel="icon" type="image/png" href="/favicon.png" />
    <meta name="SKYPE_TOOLBAR" content="SKYPE_TOOLBAR_PARSER_COMPATIBLE" />
    {% hook 'html_head_end' %}
</head>
<body>


<div class="bg-content">
  <div class="tile"></div>
</div>


<div class="content">
  <div class="line-left hidden-sm hidden-xs hidden-md"></div>
  <div class="line-right hidden-sm hidden-xs hidden-md"></div>

  <div class="top-bg"></div>
  <div class="top">

    <div class="container">

      <div class="row">

        <div class="col-xs-6">
        {% widgets 'top-left' %}
        </div>
        <div class="col-xs-6 text-right">
        {% widgets 'top-right' %}
        </div>
      </div>

    </div>

  </div>


<div class="container">
  <header>
    <a href="/" class="logo">Ресторан Гетьман</a>


    <div class="menu-icons">
      {% widgets 'menu-icons' %}
    </div>
  </header>
</div>
  <main class="container">

    <nav>
    {% widgets 'main-menu' %}
    </nav>

<div class="row">

<div class="col-xs-9">
    {{ content|raw }}
</div>
<div class="col-xs-3">
<script type="text/javascript" src="//vk.com/js/api/openapi.js?116"></script>

<!-- VK Widget -->
<div id="vk_groups"></div>
<script type="text/javascript">
VK.Widgets.Group("vk_groups", {mode: 2, width: "220", height: "400", color1: '22140e', color2: '22140e', color3: '22140e'}, 83965708);
</script>
</div>

    </div>
  </main>


</div>

      <div class="footer">Ресторан "Гетьман" &copy; Вінниця 2015</div>


	<a href="#0" class="cd-top"></a>
		{% hook 'body_end' %}
	</body>
</html>
