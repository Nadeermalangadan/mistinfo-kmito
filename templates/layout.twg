<!doctype html>
<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="ru" lang="ru">
<head profile="http://gmpg.org/xfn/11">
    {% hook 'html_head_begin' %}
    <meta http-equiv="X-UA-Compatible" content="IE=9" />
    <meta charset="utf-8">
    <meta name="SKYPE_TOOLBAR" content="SKYPE_TOOLBAR_PARSER_COMPATIBLE" />
    {% hook 'html_head_end' %}
</head>
<body>


<div class="content">
  <div class="line-left"></div>
  <div class="line-right"></div>


  <div class="container">

    <div class="top">

      <div class="row">

        <div class="col-xs-6">
        {% widgets 'top-left' %}
        </div>
        <div class="col-xs-6">
        {% widgets 'top-right' %}
        </div>
      </div>

    </div>

  </div>

</div>


<header>

<div class="container">
  <a href="/" class="logo"></a>
  </div>

</header>
<nav>
<div class="container">

  <div class="logo-small"></div>
            <div class="main-menu">
            {% widgets 'main-menu' %}
            </div>
  </div>
</nav>


<div class="container">


        <main>

            {{ content|raw }}

        </main>

</div>


<footer>
    <nav class="container">

                <div class="footer">
                {% widgets 'footer-menu' %}
                </div>
    </nav>
</footer>




		{% hook 'body_end' %}
	</body>
</html>
